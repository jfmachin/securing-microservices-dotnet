@model Movies.Client.Models.UserInfoViewModel

@{
    ViewData["Title"] = "Only admin";
}

<h1>Only Admin</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Claim type
            </th>
            <th>
                Claim value
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.UserInfoDictionary)
       {
           <tr>
               <td>
                   @Html.DisplayFor(modelItem => item.Key)
               </td>
               <td>
                   @Html.DisplayFor(modelItem => item.Value)
               </td>
           </tr>
       }
    </tbody>
</table>